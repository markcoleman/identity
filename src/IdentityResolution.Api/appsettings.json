{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "IdentityResolution": {
    "MatchingThreshold": 0.6,
    "AutoMergeThreshold": 0.97,
    "ReviewThreshold": 0.90,
    "EnableFuzzyMatching": true,
    "MaxResults": 10,
    "FieldWeights": {
      "FirstName": 0.2,
      "LastName": 0.25,
      "Email": 0.3,
      "Phone": 0.15,
      "DateOfBirth": 0.1
    }
  }
}